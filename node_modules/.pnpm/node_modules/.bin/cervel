#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tiru/Documents/GitHub/portfolio/node_modules/.pnpm/@vercel+cervel@0.0.19_rollup@4.57.1_typescript@5.9.3/node_modules/@vercel/cervel/bin/node_modules:/home/tiru/Documents/GitHub/portfolio/node_modules/.pnpm/@vercel+cervel@0.0.19_rollup@4.57.1_typescript@5.9.3/node_modules/@vercel/cervel/node_modules:/home/tiru/Documents/GitHub/portfolio/node_modules/.pnpm/@vercel+cervel@0.0.19_rollup@4.57.1_typescript@5.9.3/node_modules/@vercel/node_modules:/home/tiru/Documents/GitHub/portfolio/node_modules/.pnpm/@vercel+cervel@0.0.19_rollup@4.57.1_typescript@5.9.3/node_modules:/home/tiru/Documents/GitHub/portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tiru/Documents/GitHub/portfolio/node_modules/.pnpm/@vercel+cervel@0.0.19_rollup@4.57.1_typescript@5.9.3/node_modules/@vercel/cervel/bin/node_modules:/home/tiru/Documents/GitHub/portfolio/node_modules/.pnpm/@vercel+cervel@0.0.19_rollup@4.57.1_typescript@5.9.3/node_modules/@vercel/cervel/node_modules:/home/tiru/Documents/GitHub/portfolio/node_modules/.pnpm/@vercel+cervel@0.0.19_rollup@4.57.1_typescript@5.9.3/node_modules/@vercel/node_modules:/home/tiru/Documents/GitHub/portfolio/node_modules/.pnpm/@vercel+cervel@0.0.19_rollup@4.57.1_typescript@5.9.3/node_modules:/home/tiru/Documents/GitHub/portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@vercel/cervel/bin/cervel.mjs" "$@"
else
  exec node  "$basedir/../@vercel/cervel/bin/cervel.mjs" "$@"
fi
